{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "strict": false,
    "resolveJsonModule": true,
    "types": ["cypress"],
    "baseUrl": "."
  },
  "include": [
    "cypress.config.ts",
    "cypress/**/*.ts",
    "cypress/**/*.tsx"
  ],
  "ts-node": {
    "esm": true,
    "transpileOnly": true
  }
}