{
  "name": "cypress-aurora-reporter",
  "version": "1.0.0",
  "description": "Advanced Cypress test reporting with real-time dashboard, screenshots, and comprehensive analytics",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "npm run clean && npm run build:plugin && npm run build:dashboard",
    "build:plugin": "rollup -c rollup.config.js",
    "build:dashboard": "cd src/dashboard && npm run build",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist/types",
    "clean": "rimraf dist",
    "dev": "concurrently \"npm run dev:plugin\" \"npm run dev:dashboard\" \"npm run dev:server\"",
    "dev:plugin": "rollup -c rollup.config.js --watch",
    "dev:dashboard": "cd src/dashboard && npm run dev",
    "dev:server": "nodemon src/server/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.{ts,tsx} --fix",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint"
  },
  "keywords": [
    "cypress",
    "testing",
    "reporter",
    "dashboard",
    "screenshots",
    "mocha",
    "typescript",
    "react"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lasithdilshan20/cypress-aurora-reporter.git"
  },
  "bugs": {
    "url": "https://github.com/lasithdilshan20/cypress-aurora-reporter/issues"
  },
  "homepage": "https://github.com/lasithdilshan20/cypress-aurora-reporter#readme",
  "peerDependencies": {
    "cypress": ">=10.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "socket.io": "^4.7.4",
    "sqlite3": "^5.1.6",
    "sharp": "^0.32.6",
    "uuid": "^9.0.1",
    "date-fns": "^2.30.0",
    "fs-extra": "^11.1.1",
    "puppeteer": "^21.5.2",
    "jspdf": "^2.5.1",
    "html2canvas": "^1.4.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "concurrently": "^8.2.2",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.5.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ]
}